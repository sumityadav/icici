    "description": "ICICI bank payment gateway library.",
    "keywords": [
        "icici",
        "payseal",
        "moto",
        "payment gateway",
        "payment processing"
    ],
    "homepage": "https://github.com/sumityadav/icici",
    "license": "MIT",
    "authors": [
        {
            "name": "sumityadav",
            "email": "sumi.yadav@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Sumityadav\\Icici\\": "src"
        }
    },
    "autoload-dev": {
